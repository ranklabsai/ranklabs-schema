{
  "name": "@ranklabs/schema-hydrogen",
  "version": "1.0.3",
  "description": "React components for rendering Schema.org JSON-LD (@graph) in Shopify Hydrogen using @ranklabs/schema.",
  "keywords": [
    "schema",
    "schema.org",
    "jsonld",
    "json-ld",
    "structured-data",
    "seo",
    "aeo",
    "answer-engine-optimization",
    "llm",
    "ai",
    "shopify",
    "hydrogen",
    "react",
    "rich-results",
    "product-schema",
    "faq-schema",
    "schema-markup",
    "ldjson"
  ],
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ranklabsai/ranklabs-schema.git",
    "directory": "packages/hydrogen"
  },
  "homepage": "https://ranklabs.ai",
  "bugs": {
    "url": "https://github.com/ranklabsai/ranklabs-schema/issues"
  },
  "author": "RankLabs <sang@ranklabs.ai>",
  "funding": {
    "type": "url",
    "url": "https://ranklabs.ai"
  },
  "readme": "README.md",
  "engines": {
    "node": ">=20.0.0"
  },
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "module": "./dist/index.js",
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "lint": "tsc -p tsconfig.json --noEmit",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@types/react": "^19.0.0"
  },
  "dependencies": {
    "@ranklabs/schema": "^1.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@shopify/hydrogen": ">=2025.x",
    "react": ">=18"
  },
  "peerDependenciesMeta": {
    "@shopify/hydrogen": { "optional": true }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ]
